/* uni.scss */
.content {
  padding: 40px;
  box-sizing: border-box;
}
.content .cont {
  background: #FFFFFF;
  box-shadow: 0px 4px 4px 1px rgba(204, 204, 204, 0.25);
  border-radius: 8px 8px 8px 8px;
  padding: 30px;
  box-sizing: border-box;
  text-align: center;
}
.content .cont .title {
  height: 28px;
  font-size: 20px;
  font-family: PingFang SC-Heavy, PingFang SC;
  font-weight: 800;
  color: #333333;
  line-height: 28px;
  margin-bottom: 10px;
}
.content .cont image {
  width: 150px;
  height: 150px;
  margin-bottom: 8px;
}
.content .cont .one {
  height: 40px;
  background: #4BB4EF;
  border-radius: 90px 90px 90px 90px;
  font-size: 14px;
  font-family: PingFang SC-Medium, PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
  line-height: 40px;
  text-align: center;
  margin-bottom: 20px;
}
.content .cont .two {
  height: 40px;
  background: #79ABF8;
  border-radius: 90px 90px 90px 90px;
  font-size: 14px;
  font-family: PingFang SC-Medium, PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
  line-height: 40px;
  text-align: center;
  margin-bottom: 20px;
}
.content .cont .three {
  height: 40px;
  border-radius: 90px 90px 90px 90px;
  border: 1px solid #4BB4EF;
  box-sizing: border-box;
  font-size: 14px;
  font-family: PingFang SC-Medium, PingFang SC;
  font-weight: 500;
  color: #4BB4EF;
  line-height: 40px;
}

